  "scripts": {
    "dev": "next dev -p 3100",
    "start": "next start -p 3100",
    "build": "yarn run build:next && yarn run build:widgets",
    "build:next": "next build",
    "build:widgets": "browserify -t babelify widgets/index.js | uglifyjs > dist/widgets.js",
